{
  "kind": "collectionType",
  "collectionName": "sessions",
  "info": {
    "singularName": "session",
    "pluralName": "sessions",
    "displayName": "Session",
    "description": "User Session Management"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "token": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "ipAddress": {
      "type": "string"
    },
    "userAgent": {
      "type": "text"
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "expiresAt": {
      "type": "datetime",
      "required": true
    },
    "lastActivity": {
      "type": "datetime"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::pam-user.pam-user",
      "inversedBy": "sessions"
    }
  }
}
